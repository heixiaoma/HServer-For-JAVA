log.name=app
log.rootLevel=info
log.dir=./log
log.console=true
#taskPool定时任务线程池子配置，默认大小是cpu核心数+1
taskPool=20
#开启访问统计
statistics=false
#统计规则
statisticalRules=/hel.*,/admin/.*
#-----------------集群-------------
#是否开启集群模式
app.cloud.open=false
#注册中心端口
app.cloud.port=9527
#是否是主机
app.cloud.master.open=false
#是否是从机
app.cloud.slave.open=false
#从机连接主机的IP地址
app.cloud.slave.master.host=127.0.0.1
#从机的名字
app.cloud.slave.name=ServerA
